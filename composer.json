{
    "name": "ana/proyecto_unidad3_dwp",
    "description": "Sistema de biblioteca con autenticaci√≥n",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ana Urban",
            "email": "anadarielurban@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.6",
        "swiftmailer/swiftmailer": "^6.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "php artisan optimize"
        ],
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}